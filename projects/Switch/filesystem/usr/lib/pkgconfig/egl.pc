prefix=/usr
libdir=${prefix}/lib
includedir=${prefix}/include

Name: egl
Description: Mesa EGL Library
Version: 18.2.0-devel
Requires.private: x11, xext, xdamage >= 1.1, xfixes, x11-xcb, xcb, xcb-glx >= 1.8.1, libdrm >= 2.4.75, xxf86vm, xcb-dri2 >= 1.8
Libs: -L${libdir} -lEGL
Libs.private: -lpthread -pthread -lm -ldl
Cflags: -I${includedir}
